<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://Java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Project1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
   <servlet>
  		<servlet-name>LoginServlet</servlet-name>
  		<servlet-class>com.example.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/logining</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  		<servlet-name>SignUpServlet</servlet-name>
  		<servlet-class>com.example.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignUpServlet</servlet-name>
  	<url-pattern>/signuping</url-pattern>
  </servlet-mapping>


  <servlet>
  		<servlet-name>LoginServlet2</servlet-name>
  		<servlet-class>com.example.LoginServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet2</servlet-name>
  	<url-pattern>/logining2</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>LoginServlet3</servlet-name>
  		<servlet-class>com.example.LoginServlet3</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet3</servlet-name>
  	<url-pattern>/logining3</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  		<servlet-name>newticket</servlet-name>
  		<servlet-class>com.example.newticketservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>newticket</servlet-name>
  	<url-pattern>/newticket</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>JSON</servlet-name>
  		<servlet-class>com.example.JSONservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSON</servlet-name>
  	<url-pattern>*.json</url-pattern>
  </servlet-mapping>
  
   <servlet>
  		<servlet-name>approve</servlet-name>
  		<servlet-class>com.example.approveservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>approve</servlet-name>
  	<url-pattern>/approve</url-pattern>
  </servlet-mapping>
  
   <servlet>
  		<servlet-name>deny</servlet-name>
  		<servlet-class>com.example.denyservlet</servlet-class>
  </servlet>
  <servlet-mapping>
